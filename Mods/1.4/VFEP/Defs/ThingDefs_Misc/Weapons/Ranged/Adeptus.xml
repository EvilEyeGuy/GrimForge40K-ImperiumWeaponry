<Defs>
    <!-- WARCASKET WEAPONS -->
    <ThingDef ParentName="VFEP_BaseWarcasketGun">
        <defName>GF40K_HeavyBoltGun</defName>
        <label>Heavy bolt gun</label>
        <description>.75 caliber, 6 rounds, long range, single shot.</description>
        <graphicData>
            <texPath>Things/Item/Equipment/WeaponRanged/SniperRifle</texPath>
        </graphicData>
        <soundInteract>Interact_Rifle</soundInteract>
        <statBases>
            <WorkToMake>90000</WorkToMake>
            <Mass>59</Mass>
            <AccuracyTouch>0.56</AccuracyTouch>
            <AccuracyShort>0.65</AccuracyShort>
            <AccuracyMedium>0.72</AccuracyMedium>
            <AccuracyLong>0.68</AccuracyLong>
            <RangedWeapon_Cooldown>3</RangedWeapon_Cooldown>
        </statBases>
        <weaponTags>
            <li>WarcasketVeteran</li>
            <li>WarcasketBasic</li>
        </weaponTags>
        <costList>
            <Steel>300</Steel>
            <Plasteel>80</Plasteel>
            <ComponentSpacer>3</ComponentSpacer>
        </costList>
        <verbs>
            <li>
                <verbClass>Verb_Shoot</verbClass>
                <hasStandardCommand>true</hasStandardCommand>
                <defaultProjectile>GF40K_HeavyBoltShot</defaultProjectile>
                <warmupTime>2</warmupTime>
                <range>36</range>
                <burstShotCount>1</burstShotCount>
                <soundCast>Shot_Autocannon</soundCast>
                <soundCastTail>GunTail_Heavy</soundCastTail>
                <muzzleFlashScale>9</muzzleFlashScale>
                <forcedMissRadius>0.9</forcedMissRadius>
            </li>
        </verbs>
        <tools>
            <li>
                <label>handle</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>11</power>
                <cooldownTime>3</cooldownTime>
            </li>
            <li>
                <label>front</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>9</power>
                <cooldownTime>3</cooldownTime>
            </li>
        </tools>
    </ThingDef>
